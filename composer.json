{
    "name": "creode/laravel-nova-meet-the-team",
    "description": "Add Meet the Team feature to your Laravel Nova application.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "creode/laravel-repository": "^1.0",
        "creode/nova-publishable": "^1.1",
        "laravel/nova": "^4.32",
        "outl1ne/nova-sortable": "^3.4",
        "spatie/laravel-package-tools": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelNovaMeetTheTeam\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelNovaMeetTheTeam\\LaravelNovaMeetTheTeamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/canvas": "^8.11",
        "orchestra/testbench": "^8.8"
    }
}
